;(function (modules) {
    //* 1.实现 require()
    function require(filePath) {
        const fn = modules[filePath]

        const module = {
            exports: {}
        }

        fn(require, module, module.exports)
        return module.exports
    }

    //* 2.执行入口文件
    require('<%- data[0].filePath %>')
})({
    <% data.forEach((d) => { %>
    '<%- d.filePath %>': function (require, module, exports) {
        <%- d.code %>
    },
    <%}) %>
});
